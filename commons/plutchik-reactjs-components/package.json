{
    "name": "plutchik-reactjs-components",
    "version": "1.0.0",
    "description": "Plutchik emotional azimuth",
    "main": "./dist/cjs/index.js",
    "module": "./dist/esm/index.js",
    "types": "./dist/esm/index.d.ts",
    "scripts": {
        "build:esm": "cp **/*.css dist/esm && npx tsc",
        "build:cjs": "cp **/*.css dist/cjs && npx tsc --module commonjs --outDir dist/cjs",
        "build": "npm run build:esm && npm run build:cjs"
    },
    "author": "Pavel Komov",
    "license": "ISC",
    "publishConfig": {
        "access": "public"
    },
    "peerDependencies": {
        "react": "^18.3.1",
        "react-dom": "^18.3.1"
    },
    "devDependencies": {
        "@types/react": "^18.3.3",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "typescript": "^5.5.4"
    },
    "files": [
        "dist",
        "LICENSE",
        "README.md"
    ]
}
